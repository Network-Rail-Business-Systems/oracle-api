{
    "name": "networkrailbusinesssystems/oracle-api",
    "description": "Provide end points for sending and receiving data from Oracle (iStore Catalogue)",
    "type": "library",
    "homepage": "https://github.com/Network-Rail-Business-Systems/oracle-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Ambreen Jamal",
            "email": "ambreen.jamal@networkrail.co.uk"
        }
    ],
    "require": {
        "php": "^8",
        "guzzlehttp/guzzle": "^7",
        "illuminate/support": "^10"
    },
    "require-dev": {
        "laravel/pint": "^1.7",
        "orchestra/testbench": "^8",
        "phpunit/phpunit": "^10"
    },
    "autoload": {
        "psr-4": {
            "NetworkRailBusinessSystems\\OracleApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NetworkRailBusinessSystems\\OracleApi\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NetworkRailBusinessSystems\\OracleApi\\OracleApiServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
